{
  "name": "starter-web",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "oxlint",
    "lint:fix": "oxlint && oxfmt",
    "up": "taze major -I",
    "fmt": "oxfmt",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "lint-staged": "^16.2.7",
    "oxfmt": "^0.24.0",
    "oxlint": "^1.39.0",
    "simple-git-hooks": "^2.13.1",
    "taze": "^19.9.2",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vue-tsc": "^3.1.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "pnpm run lint:fix"
  }
}
